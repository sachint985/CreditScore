<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>check-score</servlet-name>
  <servlet-class>com.creditscore.servlets.CheckScore</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>eligible</servlet-name>
  <jsp-file>/eligible.jsp</jsp-file>
  </servlet>
  <servlet>
  <servlet-name>not-eligible</servlet-name>
  <jsp-file>/not-eligible.jsp</jsp-file>
  </servlet>
  <servlet>
  <servlet-name>invalid</servlet-name>
  <jsp-file>/invalid.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>check-score</servlet-name>
  	<url-pattern>/checkScore</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>eligible</servlet-name>
  	<url-pattern>/Eligible</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>not-eligible</servlet-name>
  	<url-pattern>/NotEligible</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>invalid</servlet-name>
  	<url-pattern>/Invalid</url-pattern>
  </servlet-mapping>
</web-app>
